<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<config>
    <lines at="topleft">
        <line>
            <str>Day {day}, {mctime12} (</str>
            <if>
                <var>daytime</var>
                <str>$eDay</str>
                <str>$8Night</str>
            </if>
            <str> time$f)</str>
        </line>
        <line>
            <str>Light: </str>
            <max>
                <var>light</var>
                <num>7.5</num>
                <str>$e</str>
                <str>$c</str>
            </max>
            <var>light</var>
            <str>$r</str>
        </line>
        <line>
            <str>Coords: $e{xi}$f, $e{yi}$f, $e{zi}$f</str>
        </line>
        <line>
            <str>Direction: $e{roughdirection}$f</str>
        </line>
        <line>
            <str>Biome: $e{biome}$f (ID: $e{biomeid}$f)</str>
        </line>
        <line>
            <str>$fEXP: $e{xpthislevel}$f / $e{xpcap}$f ($e</str>
            <round>
                <pct>
                    <var>xpthislevel</var>
                    <var>xpcap</var>
                </pct>
                <num>0</num>
            </round>
            <str>%$f)</str>
        </line>
        <line>
            <str>FPS: </str>
            <op>
                <str>gt</str>
                <var>fps</var>
                <num>100</num>
                <num>70</num>
                <num>45</num>
                <num>20</num>
                <num>0</num>
                <str>$a</str>
                <str>$e</str>
                <str>$6</str>
                <str>$c</str>
                <str>$4</str>
                <str/>
            </op>
            <var>fps</var>
        </line>
        <line>
            <str>Entities: $e{entitiesrendered}$f / $e{entitiestotal}$f</str>
        </line>
    </lines>
    <lines at="middleright">
